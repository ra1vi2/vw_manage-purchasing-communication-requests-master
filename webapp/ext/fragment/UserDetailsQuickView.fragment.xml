<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<QuickView id="idQuickView">
		<QuickViewPage id="idQuickViewPage" icon="sap-icon://person-placeholder" title="{quickViewModel>/FullName}">
			<QuickViewGroup id="idQuickViewGroup">
				<QuickViewGroupElement id="idQuickViewElementEmail" label="{i18n>Email}" value="{quickViewModel>/Email}" type="email"/>
				<QuickViewGroupElement id="idQuickViewElementDepartment" label="{i18n>Department}" value="{quickViewModel>/Department}" type="text"/>
			</QuickViewGroup>
		</QuickViewPage>
	</QuickView>
</core:FragmentDefinition>